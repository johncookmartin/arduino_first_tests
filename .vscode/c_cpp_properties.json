{
  "version": 4,
  "configurations": [
    {
      "name": "WSL-AVR",
      "compilerPath": "/usr/bin/avr-gcc",
      "compilerArgs": ["-mmcu=atmega328p"],
      "intelliSenseMode": "linux-gcc-x64",
      "cStandard": "c11",
      "cppStandard": "c++17",
      "defines": ["F_CPU=16000000UL"],
      "includePath": [
        "${workspaceFolder}",
        "/usr/lib/avr/include",
        "/usr/lib/gcc/avr/**"
      ]
    }
  ]
}
